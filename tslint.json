{
  "rules": {
    "ordered-imports": [false],
    "semicolon": [false, "always", "ignore-interfaces"],
    "member-access": [false],
    "member-ordering": [false],
    "trailing-comma": [
      true,
      {
        "multiline": {
          "objects": "always",
          "arrays": "always",
          "functions": "always",
          "typeLiterals": "ignore"
        },
        "singleline": "never"
      }
    ],
    "no-empty": [false],
    "max-classes-per-file": [false],
    "no-empty-interface": [false],
    "quotemark": [true, "single", "jsx-double"]
  }
}
